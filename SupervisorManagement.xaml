<Window x:Class="yes.SupervisorManagement"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:yes"
        mc:Ignorable="d"
        Title="SupervisorManagement" Height="460" Width="831">
    <Grid>
        <TextBlock Text="Управление должностями" FontSize="20" FontWeight="Bold" Margin="10,10,10,372" TextAlignment="Center" RenderTransformOrigin="0.506,1.366"/>

        <Button x:Name="Voiti" Content="Назад" Grid.Row="0" Margin="0,377,0,0" RenderTransformOrigin="0.508,1.154" Height="45" VerticalAlignment="Top" HorizontalAlignment="Center" Width="261" Click="Voiti_Click"/>
        <Button x:Name="Delete" Content="Удалить" Grid.Row="0" Margin="0,310,0,0" RenderTransformOrigin="0.508,1.154" Height="34" VerticalAlignment="Top" HorizontalAlignment="Center" Width="157" Click="Delete_Click"/>
        <Button x:Name="Add" Content="Добавить" Grid.Row="0" Margin="156,310,0,0" RenderTransformOrigin="0.508,1.154" Height="34" VerticalAlignment="Top" HorizontalAlignment="Left" Width="157" Click="Add_Click"/>
        <Button x:Name="Edit" Content="Редактировать" Grid.Row="0" Margin="508,310,0,0" RenderTransformOrigin="0.508,1.154" Height="34" VerticalAlignment="Top" HorizontalAlignment="Left" Width="157" Click="Edit_Click"/>
        <DataGrid x:Name="spisok" Margin="0,72,448,0" HorizontalAlignment="Right" Width="323" Height="224" VerticalAlignment="Top"/>
        <TextBox x:Name="Name" Margin="483,142,0,0" HorizontalAlignment="Left" Width="261" Height="32" VerticalAlignment="Top"/>
    </Grid>
</Window>
