<Window x:Class="yes.MainLaborant"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:yes"
        mc:Ignorable="d"
        Title="MainLaborant" Height="450" Width="800">
    <Grid>
        <TextBlock Text="Лаборант" FontSize="20" FontWeight="Bold" Margin="10,10,10,372" TextAlignment="Center" RenderTransformOrigin="0.506,1.366"/>
        <Button x:Name="Users" Content="Анализы пациента"   Margin="0,110,0,0" RenderTransformOrigin="0.511,1.526" HorizontalAlignment="Center" Width="240" Height="35" VerticalAlignment="Top" Click="Users_Click"></Button>
        <Button x:Name="Orders" Content="Анализы" Grid.Row="0" Margin="0,67,0,0" HorizontalAlignment="Center" Width="240" Height="28" VerticalAlignment="Top" Click="Orders_Click"/>
        <Button x:Name="Voiti" Content="Выйти из аккаунта"  Background="#FFFB4646" Margin="280,355,0,0" Height="35" VerticalAlignment="Top" HorizontalAlignment="Left" Width="240" Click="Voiti_Click"/>
    </Grid>
</Window>
